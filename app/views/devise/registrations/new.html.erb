<div class="row">
  <div class="span6">

    <%= simple_form_for(resource, :as => resource_name, :url => registration_path(resource_name), html: {class: "well"}) do |f| %>
      <%= devise_error_messages! %>
        <fieldset>
            <legend>Register</legend>
          	<div><%= f.input :first_name %></div>
            <div><%= f.input :last_name %></div>
            <div><%= f.input :profile_name %></div>
            <div><%= f.input :email %></div>
          <% if f.object.password_required? %>
            <div><%= f.input :password %></div>
            <div><%= f.input :password_confirmation %></div>
          <% end %>

          <div><%= f.button :submit, "Submit" %></div>
        </fieldset>
    <% end %>
  </div>

  <div class="span6">
    <div class="well">
      <legend>Use Another Account</legend>
        <%= link_to raw('<i class="icon-twitter-sign icon-4x"></i>'), user_omniauth_authorize_path(:twitter) %>
        <%= link_to raw('<i class="icon-facebook-sign icon-4x"></i>'), user_omniauth_authorize_path(:facebook) %>
        <%= link_to raw('<i class="icon-google-plus-sign icon-4x"></i>'), user_omniauth_authorize_path(:google_oauth2) %>
    </div>
  </div>


</div>
    <%= render "devise/shared/links" %>
